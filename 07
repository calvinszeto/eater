		<script type="text/javascript">
		var addthis_config = {
			username: 'curbed',
			data_track_linkbacks: true
		};
		var addthis_share = { 
			url: "http://houston.eater.com/archives/2014/01/07/the-essential-38-houston-restaurants-january-2014-1.php",
			title: "The Essential 38 Houston Restaurants, January 2014",
			email_template: "curbed_network_template",
			email_vars: { 
				site_name: "Eater Houston",
				site_url: "http://houston.eater.com/",
				post_excerpt: "It\'s time to update the Eater 38, your answer and ours to any question that begins, \"Can you recommend a restaurant?\" This highly elite group covers the entire city, spans myriad cuisines, and collectively satisfies all of your restaurant needs,..."
			} 
